{
    "name": "hapic",
    "version": "2.0.0-alpha.9",
    "description": "A http api client based on axios.",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "tsc  -p tsconfig.build.json --emitDeclarationOnly",
        "build:js": "rollup -c",
        "build": "rimraf dist && npm run build:types && npm run build:js",
        "prepublishOnly": "npm run build",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "npm run test -- --coverage"
    },
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "keywords": [
        "query",
        "json",
        "json-api",
        "api",
        "rest",
        "api-utils",
        "include",
        "pagination",
        "sort",
        "fields",
        "relations",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/hapic.git"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/hapic/issues"
    },
    "homepage": "https://github.com/Tada5hi/hapic#readme",
    "dependencies": {
        "ebec": "^1.0.0",
        "node-fetch-native": "^1.1.0",
        "ufo": "^1.1.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "ac770f3f6e1ec2fd011cdf432b06319c635c01fa"
}
